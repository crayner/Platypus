<!-- Pagination Script -->
{% trans_default_domain pagination.transDomain %}
<script>
    {% set props = pagination.displayProperties %}
    {% set translations = {} %}
    {% for value in props['translations'] %}
        {% set translations = translations|merge({(value): value|trans({}, 'Pagination')}) %}
    {% endfor %}
    {% for value in props['specificTranslations'] %}
        {% set translations = translations|merge({(value): value|trans })|raw %}
    {% endfor %}
    {% set props = props|merge({translations: translations}) %}
    window.PAGINATION_PROPS = {{ props|json_encode|raw }}
</script>
{{ encore_entry_script_tags('pagination') }}
