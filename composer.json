{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.0|^7.2.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofsymfony/ckeditor-bundle": "^1.1",
        "hillrange/security": "^0.0",
        "hillrange/symfony-form": "^0.3",
        "knpuniversity/oauth2-client-bundle": "^1.16",
        "league/oauth2-google": "^2.2",
        "php-translation/symfony-bundle": "^0.8",
        "platypus-template/original": "^0.0",
        "symfony/asset": "^4.1|^4.2",
        "symfony/console": "^4.1|^4.2",
        "symfony/debug": "^4.1|^4.2",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.1|^4.2",
        "symfony/framework-bundle": "^4.1|^4.2",
        "symfony/intl": "^4.1|^4.2",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.1|^4.2",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/templating": "^4.1|^4.2",
        "symfony/twig-bridge": "^4.1|^4.2",
        "symfony/twig-bundle": "^4.1|^4.2",
        "symfony/validator": "^4.1|^4.2",
        "symfony/var-dumper": "^4.1|^4.2",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "^4.1|^4.2",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.4"
    },
    "require-dev": {
        "symfony/dotenv": "^4.1|^4.2",
        "symfony/maker-bundle": "^1.5",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "application:install --relative --symlink ": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "php -r \"copy('config/packages/platypus.yaml.dist', 'config/packages/platypus.yaml');\"",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
